        "subdomains": "abcd"
      }
    },
	"geoSearch": true
  },
  "customBaseMap": {
    "url": "http://localhost:8080/hydrovise/examples/ex1_min/data/gis_data/customBaseMap.geojson",
    "colors": {
      "3": "#00e4ff",
      "4": "#00c5ff",
      "5": "#00acff",
      "6": "#018eff",
      "7": "#007dff",
      "8": "#007bff",
      "9": "#0055ff",
      "10": "#000000",
      "11": "#321321"
    },
    "name" : "River Network",
    "propoertyName": "h_order"
  },
  "calcMetrics":{
	"metricList":["kge","mse","nse"]
	}
}