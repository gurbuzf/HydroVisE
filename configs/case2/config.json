  "traces": {
    "t1": {
      "template": {
        "var": [
          "yr",
          "usgs_id"
        ],
        "path_format": "data/timeSeries/{0}/Q/{1}"
      },
      "dynamic": 0,
      "group": 0,
      "prod": "Q",
      "extension": ".csv",
      "stageEnabled": true,
      "x_name": "dt",
      "y_name": "Q",
      "style": {
        "type": "scattergl",
        "mode": "lines",
        "name": "USGS",
        "line": {
          "width": 2,
          "color": "black"
        }
      }
    },
    "t2": {
      "root": "http://s-iihr50.iihr.uiowa.edu/smap/retro/ltime_data/",
      "template": {
        "var": [
          "yr",
          "prod",
          "usgs_id",
          "sliderState"
        ],
        "path_format": "{0}/{1}/{2}/{3}",
        "_sliderState": {
          "arguments": "v",
          "body": "return ('000' + String(v)).slice(-3);"
        }
      },
      "dynamic": 1,
      "group": 1,
      "prod": "Q_ifc_nwm",
      "extension": ".csv",
      "stageEnabled": true,
      "x_name": "dt",
